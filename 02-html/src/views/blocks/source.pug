section.source
  .source__container.container
    .source__heading
      if isStructureVisible
        ul.source__tabs-list.list
          li.source__tab-item.list-item
            a(href='inner.html').source__tab.tab.tab_text_uppercase.tab_color_grey.text.text_size_m.link
              span.tab__name.tab__name_current.tab__name_space_s Files
          li.source__tab-item.list-item
            a(href='branches.html').source__tab.tab.tab_text_uppercase.tab_color_grey.text.text_size_m.link
              span.tab__name.tab__name_space_s Branches
      if !isStructureVisible || isHistoryVisible
        ul.source__tabs-list.list
          li.source__tab-item.list-item
            a(href='details.html').source__tab.tab.tab_text_uppercase.tab_color_grey.text.text_size_m.link
              span.tab__name.tab__name_current.tab__name_space_s Details
          li.source__tab-item.list-item
            a(href='history.html').source__tab.tab.tab_text_uppercase.tab_color_grey.text.text_size_m.link
              span.tab__name.tab__name_space_s History
    if isStructureVisible
      table.source__table
        tbody.source__table-body
          tr.source__table-row.source__table-row_mobile-hidden.source__table-row_heading.text.text_size_l.text_color_grey-6.text_weight_medium
            td.source__table-data
              span Name
            td.source__table-data
              span Last Commit
            td.source__table-data
              span Commit message
            td.source__table-data
              span Commiter
            td.source__table-data.source__table-data_tablet-text-right
              span Updated
          tr.source__table-row.text.text_size_l
            td.source__table-data.source__table-data_source
              a(href='inner.html').source__table-link.source__table-link_action-blue.text.text_icon_folder.text_weight_medium.link
                svg(role='img' width='12' height='9').source__table-icon.source__table-icon_left.icon
                  use(xlink:href='img/icons/sprite.svg#icon-folder')
                | api
            td.source__table-data.source__table-data_commit-hash
              a(href='#').source__table-link.text.link.link_color_blue.link_hover_underline r3248719
            td.source__table-data.source__table-data_commit-msg
              span.source__table-text.text [vcs] move http to a
            td.source__table-data.source__table-data_commiter
              a(href='#').source__table-link.text.link
