{
  "name": "git-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress:open": "npm run clone-test && cypress open",
    "server": "nodemon src/server/src/index.js --path=./src/server/repos",
    "clone-test": "rimraf ./src/server/repos/learn-unit-testing && git clone git@github.com:webistomin/learn-unit-testing.git ./src/server/repos/learn-unit-testing"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cypress-plugin-snapshots": "^1.2.9",
    "lodash.throttle": "^4.1.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-helmet": "^5.2.1",
    "react-highlight": "^0.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2"
  },
  "devDependencies": {
    "cypress": "^3.4.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "rimraf": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
